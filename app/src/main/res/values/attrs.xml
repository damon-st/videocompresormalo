<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="minTrimMs" format="integer"/>
    <attr name="thumbWidth" format="dimension" />
    <attr name="leftThumb" format="reference" />
    <attr name="rightThumb" format="reference" />
    <attr name="borderColor" format="color|reference" />

    <declare-styleable name="RangeSeekBar">
        <attr name="minTrimMs"/>
        <attr name="thumbWidth"/>
        <attr name="leftThumb"/>
        <attr name="rightThumb"/>
        <attr name="borderColor"/>
    </declare-styleable>

    <declare-styleable name="VideoEditor">
        <attr name="minTrimMs"/>
        <attr name="thumbWidth"/>
        <attr name="leftThumb"/>
        <attr name="rightThumb"/>
        <attr name="borderColor" />
        <attr name="maxThumbCount" format="integer" />
    </declare-styleable>
</resources>